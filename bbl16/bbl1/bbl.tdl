;;; Features
;;;;;;;;;;;;;;

;;; Case

case := *top*.
erg := case.  ; erg
abs := case.  ; abs
all := case.  ; all
con := case.  ; con
instr := case.  ; instr
adverbial := case.  ; adverbial
ill := case.  ; ill
dir := case.  ; dir
loc := case.  ; loc
all-loc := case.  ; all-loc
ill-loc := case.  ; ill-loc
gen+dat := case.
gen := gen+dat.
dat := gen+dat.

;;; Person

person := *top*.
1st := person.
non-1st := person.
2nd := non-1st.
3rd := non-1st.

;;; Number

number := *top*.
pl := number.
sg := number.

;;; Gender

gender := *top*.
f := gender.
m := gender.
vb := gender.
jd := gender.
jj := gender.
dd := gender.
dj := gender.
bb := gender.
bd := gender.
bj := gender.

;;; Form

form := *top*.
nonfinite := form.
finite := form.

;;; Aspect

cmpl := aspect.
cont := aspect.
pfv := aspect.
ipfv := aspect.

